{
    "name": "@sassy/tiktok-download",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "exports": {
        ".": "./src/index.ts",
        "./schema-validators": "./src/schema-validators.ts"
    },
    "license": "MIT",
    "scripts": {
        "clean": "git clean -xdf .cache .turbo dist node_modules downloads",
        "format": "prettier --check . --ignore-path ../../.gitignore",
        "lint": "eslint",
        "typecheck": "tsc --noEmit --emitDeclarationOnly false",
        "with-env": "dotenv -e ../../.env --",
        "tiktok-download": "pnpm with-env bun scripts/tiktok-download.ts",
        "process-videos": "pnpm with-env bun scripts/process-videos.ts",
        "process-videos:dry": "pnpm with-env bun scripts/process-videos.ts --dry-run"
    },
    "dependencies": {
        "@sassy/db": "workspace:*",
        "@sassy/s3": "workspace:*",
        "zod": "catalog:"
    },
    "devDependencies": {
        "@sassy/eslint-config": "workspace:*",
        "@sassy/prettier-config": "workspace:*",
        "@sassy/tsconfig": "workspace:*",
        "dotenv-cli": "^8.0.0",
        "@types/node": "^22.10.10",
        "eslint": "catalog:",
        "typescript": "catalog:"
    },
    "prettier": "@sassy/prettier-config"
}