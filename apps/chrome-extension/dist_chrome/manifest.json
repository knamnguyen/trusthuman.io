{
  "manifest_version": 3,
  "key": "",
  "name": "EngageKit",
  "description": "Automatically comment on LinkedIn posts using AI",
  "options_ui": {
    "page": "src/pages/options/index.html"
  },
  "action": {
    "default_icon": "public/dev-icon-32.png",
    "default_popup": "src/pages/popup/index.html"
  },
  "chrome_url_overrides": {
    "newtab": "src/pages/newtab/index.html"
  },
  "icons": {
    "128": "public/dev-icon-128.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "windows",
    "scripting",
    "storage",
    "cookies",
    "alarms"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/index.tsx-loader-D2Epu3nn.js",
        "assets/always-active-core.ts-l0sNRNKZ.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*",
        "<all_urls>"
      ],
      "css": [
        "contentStyle.css"
      ]
    }
  ],
  "devtools_page": "src/pages/devtools/index.html",
  "web_accessible_resources": [
    {
      "matches": [],
      "resources": [
        "contentStyle.css",
        "dev-icon-128.png",
        "dev-icon-32.png"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "<all_urls>",
        "http://*/*",
        "https://*/*"
      ],
      "resources": [
        "assets/react-CyMwB-ya.js",
        "assets/index-CqyD2INw.js",
        "assets/_commonjsHelpers-gnU0ypJ3.js",
        "assets/index.tsx-D3GIAyKc.js",
        "assets/always-active-core.ts-l0sNRNKZ.js"
      ],
      "use_dynamic_url": false
    }
  ],
  "version": "1.4.0"
}
