{
  "manifest_version": 3,
  "name": "EngageKit",
  "description": "Automatically comment on LinkedIn posts using AI",
  "action": {
    "default_icon": "public/dev-icon-32.png",
    "default_popup": "src/pages/popup/index.html"
  },
  "icons": {
    "128": "public/dev-icon-128.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "cookies",
    "alarms"
  ],
  "host_permissions": [
    "http://localhost:3000/*",
    "https://*.linkedin.com/*",
    "https://aistudio.google.com/*",
    "https://*.clerk.accounts.dev/*",
    "https://*.clerk.dev/*",
    "https://engagekit.io/*",
    "https://accounts.engagekit.io/*",
    "https://clerk.engagekit.io/*"
  ],
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/index.tsx-loader-vbJvV8YN.js",
        "assets/always-active-core.ts-l0sNRNKZ.js"
      ],
      "matches": [
        "https://*.linkedin.com/*"
      ],
      "css": [
        "contentStyle.css"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [],
      "resources": [
        "contentStyle.css",
        "dev-icon-128.png",
        "dev-icon-32.png"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "https://*.linkedin.com/*"
      ],
      "resources": [
        "assets/react-ELs9A1IF.js",
        "assets/index.tsx-D7oLTchl.js",
        "assets/always-active-core.ts-l0sNRNKZ.js"
      ],
      "use_dynamic_url": false
    }
  ],
  "version": "1.4.0",
  "key": ""
}
