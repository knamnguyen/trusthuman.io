{
  "manifest_version": 3,
  "key": "MIIBIjANBgkqhkiG9w0BAQEFAAOCAQ8AMIIBCgKCAQEAyKYRvdvPgvOBYpH6rRqQz3K8HMIm9AQs9h8IkF0xG7YpM2kKwJz6r1F9ZIb+2dT8Q3nE4cJ5HhF0gH2mJ8vN7wK9pL3oR5uS1xV6yW8z9nA0bC1d2eF3g4h5i6j7k8l9m0n1o2p3q4r5s6t7u8v9w0x1y2z3",
  "name": "EngageKit Turbo",
  "description": "Automatically comment on LinkedIn posts using AI",
  "options_ui": {
    "page": "src/pages/options/index.html"
  },
  "action": {
    "default_popup": "src/pages/popup/index.html",
    "default_icon": {
      "32": "icon-32.png"
    }
  },
  "chrome_url_overrides": {
    "newtab": "src/pages/newtab/index.html"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "windows",
    "scripting",
    "storage",
    "cookies"
  ],
  "host_permissions": [
    "https://*.linkedin.com/*",
    "https://openrouter.ai/*",
    "https://aistudio.google.com/*",
    "https://*.clerk.accounts.dev/*",
    "https://*.clerk.dev/*",
    "https://tolerant-hagfish-1.clerk.accounts.dev/*",
    "http://localhost:3000/*",
    "https://*.ngrok-free.app/*",
    "https://engagekit.io/*"
  ],
  "background": {
    "service_worker": "src/pages/background/index.ts"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*", "<all_urls>"],
      "js": [
        "src/pages/content/index.tsx",
        "src/pages/content/always-active-core.ts"
      ],
      "css": ["contentStyle.css"]
    }
  ],
  "devtools_page": "src/pages/devtools/index.html",
  "web_accessible_resources": [
    {
      "resources": ["contentStyle.css", "icon-128.png", "icon-32.png"],
      "matches": []
    }
  ]
}
