{
  "$schema": "https://turborepo.org/schema.json",
  "ui": "tui",
  "tasks": {
    "topo": {
      "dependsOn": [
        "^topo"
      ]
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".cache/tsbuildinfo.json",
        "dist/**",
        "dist_chrome/**"
      ]
    },
    "dev": {
      "dependsOn": [
        "^dev"
      ],
      "cache": false,
      "persistent": false
    },
    "format": {
      "outputs": [
        ".cache/.prettiercache"
      ],
      "outputLogs": "new-only"
    },
    "lint": {
      "dependsOn": [
        "^topo",
        "^build"
      ],
      "outputs": [
        ".cache/.eslintcache"
      ]
    },
    "typecheck": {
      "dependsOn": [
        "^topo",
        "^build"
      ],
      "outputs": [
        ".cache/tsbuildinfo.json"
      ]
    },
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    },
    "push": {
      "cache": false,
      "interactive": true
    },
    "studio": {
      "cache": false,
      "persistent": true
    },
    "ui-add": {
      "cache": false,
      "interactive": true
    },
    "copy:prisma": {
      "cache": false
    }
  },
  "globalEnv": [
    "DATABASE_URL",
    "DIRECT_URL",
    "CLERK_SECRET_KEY",
    "CLERK_WEBHOOK_SECRET",
    "CRON_SECRET",
    "NEXT_PUBLIC_CLERK_FRONTEND_API",
    "CLERK_API_KEY",
    "CLERK_JWT_KEY",
    "STRIPE_SECRET_KEY",
    "STRIPE_WEBHOOK_SECRET",
    "NEXTJS_URL",
    "PORT",
    "GEMINI_API_KEY",
    "NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY",
    "CLERK_SECRET_KEY",
    "LOOPS_API_KEY",
    "APIFY_LINKEDIN_ACTOR_ID",
    "APIFY_API_TOKEN",
    "HYPERBROWSER_API_KEY",
    "LINKEDIN_TEST_ACCOUNT_USERNAME",
    "LINKEDIN_TEST_ACCOUNT_PASSWORD",
    "LINKEDIN_TEST_ACCOUNT_2FA_SECRET_KEY",
    "LINKEDIN_PASSWORD_SECRET_KEY",
    "LINKEDIN_TWO_FA_SECRET_KEY",
    "JWT_SECRET"
  ],
  "globalPassThroughEnv": [
    "NODE_ENV",
    "CI",
    "VERCEL",
    "VERCEL_ENV",
    "VERCEL_URL",
    "npm_lifecycle_event"
  ]
}